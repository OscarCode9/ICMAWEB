<script type="text/javascript">
    const myHeaders= new Headers();
    myHeaders.append('authorization', `Bearer ${localStorage.token}`)
    fetch('/api/product',{
        method: 'GET',
        headers: myHeaders
    })
    .then( res =>res.json()
    )
    .then(data =>{
        console.log(data.mensaje)
        if(!data.mensaje){
            window.location = "/admin"
        }
    })
</script>